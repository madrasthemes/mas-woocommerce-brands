{
	"name": "madrasthemes/mas-woocommerce-brands",
	"description": "Brands plugin for WooCommerce by MadrasThemes.",
	"homepage": "https://madrasthemes.com/",
	"type": "wordpress-plugin",
	"license": "GPL-3.0+",
	"require": {
		"composer/installers": "2.0.1"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "3.6.2",
		"wp-coding-standards/wpcs": "2.3.0",
		"woocommerce/woocommerce-sniffs": "0.1.1",
		"phpcompatibility/php-compatibility": "9.3.5",
		"woocommerce/woocommerce-git-hooks": "1.0.5",
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.0"
	},
	"scripts": {
		"phpcs": [
			"phpcs --extensions=php -s -p"
		],
		"phpcbf": [
			"phpcbf --extensions=php -p"
		]
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
